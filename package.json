{
  "name": "vox-mind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "dev:worker": "wrangler dev --local",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "wrangler pages deploy out/ --project-name=vox-mind",
    "db:migrate:local": "wrangler d1 migrations apply vox-mind-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply vox-mind-db --remote",
    "db:execute:local": "wrangler d1 execute vox-mind-db --local",
    "db:execute:remote": "wrangler d1 execute vox-mind-db --remote"
  },
  "dependencies": {
    "aws4fetch": "^1.0.20",
    "hono": "^4.6.14",
    "next": "15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241231.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wrangler": "^3.96.0"
  }
}
